{
  "name": "spectreweave5",
  "version": "0.1.0",
  "description": "SpectreWeave5 - Advanced TipTap v3.x Block Editor Application",
  "private": true,
  "scripts": {
    "dev": "node --max-old-space-size=4096 node_modules/next/dist/bin/next dev",
    "build": "node --max-old-space-size=4096 node_modules/next/dist/bin/next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write --ignore-path .gitignore \"./**/*.{js,jsx,ts,tsx,json,css}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest src/",
    "test:integration": "jest src/test/integration/",
    "test:e2e": "playwright test tests/children-books-workflow.spec.ts",
    "test:e2e:ui": "playwright test tests/children-books-workflow.spec.ts --ui",
    "test:all": "npm run test && npm run test:e2e",
    "test:a11y": "playwright test tests/accessibility.spec.ts",
    "test:a11y:ui": "playwright test tests/accessibility.spec.ts --ui",
    "test:a11y:report": "playwright show-report",
    "clip:start": "cd src/services/openclip && bash deploy-local.sh start development",
    "clip:stop": "cd src/services/openclip && bash deploy-local.sh stop",
    "clip:status": "cd src/services/openclip && bash deploy-local.sh status",
    "clip:logs": "cd src/services/openclip && bash deploy-local.sh logs",
    "clip:test": "cd src/services/openclip && bash deploy-local.sh test",
    "clip:build": "cd src/services/openclip && bash deploy-local.sh build",
    "clip:monitoring": "cd src/services/openclip && bash deploy-local.sh start monitoring",
    "clip:production": "cd src/services/openclip && bash deploy-local.sh start production",
    "clip:cleanup": "cd src/services/openclip && bash deploy-local.sh cleanup",
    "azure:deploy": "cd src/services/openclip && bash azure/deploy.sh",
    "azure:cleanup": "cd src/services/openclip && bash azure/deploy.sh --cleanup"
  },
