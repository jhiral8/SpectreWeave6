# Database Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# AI Service Configuration
GEMINI_API_KEY=your-gemini-api-key
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
AZURE_OPENAI_API_KEY=your-azure-openai-key
AZURE_OPENAI_DEPLOYMENT_NAME=your-deployment-name
DATABRICKS_API_URL=your-databricks-url
DATABRICKS_API_TOKEN=your-databricks-token

# Stability AI (for image generation)
STABILITY_API_KEY=your-stability-api-key

# Collaboration (Ably)
ABLY_API_KEY=your-ably-api-key

# Neo4j Configuration (if using GraphRAG)
NEO4J_URI=neo4j://localhost:7687
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=your-neo4j-password

# OpenCLIP Service Configuration
CLIP_SERVICE_URL=http://localhost:5000
CLIP_HEALTH_CHECK_TIMEOUT=30000

# Security
NEXTAUTH_SECRET=your-nextauth-secret
NEXTAUTH_URL=https://yourdomain.com

# Development
NODE_ENV=production